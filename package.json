    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "methods": "^1.1.2",
    "minimist": "^1.2.0",
    "mongoose": "^5.9.0",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
